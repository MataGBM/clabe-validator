{
   "name": "clabe-validator",
   "version": "1.5.0",
   "description": "JavaScript library to analyze or create a CLABE number for a Mexican bank account",
   "license": "MIT",
   "main": "dist/clabe.js",
   "module": "dist/clabe.es.js",
   "browser": "dist/clabe.min.js",
   "types": "dist/clabe.d.ts",
   "files": [
      "dist"
   ],
   "repository": "github:center-key/clabe-validator",
   "homepage": "https://github.com/center-key/clabe-validator",
   "bugs": "https://github.com/center-key/clabe-validator/issues",
   "docs": "https://centerkey.com/clabe",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "bank-account",
      "bank",
      "clabe-number",
      "clabe-validator",
      "clabe",
      "financial",
      "javascript",
      "mexico",
      "typescript",
      "validator"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "varstmt": true,
      "browser": true,
      "mocha": true,
      "node": true
   },
   "scripts": {
      "pretest": "jshint *.js && tsc --version && tsc --project . && gulp lint-html",
      "test": "gulp make-dist && mocha spec.js && env specMode=minified mocha spec.js"
   },
   "engines": {
      "node": ">=10.0.0"
   },
   "dependencies": {},
   "devDependencies": {
      "@babel/core": "~7.12",
      "@babel/preset-env": "~7.12",
      "@fortawesome/fontawesome-free": "~5.15",
      "babel-minify": "~0.5",
      "dna.js": "~1.6",
      "gulp": "~4.0",
      "gulp-append-prepend": "~1.0",
      "gulp-babel": "~8.0",
      "gulp-header": "~2.0",
      "gulp-htmlhint": "~4.0",
      "gulp-rename": "~2.0",
      "gulp-replace": "~1.0",
      "gulp-size": "~3.0",
      "gulp-w3c-html-validator": "~2.0",
      "jquery": "~3.5",
      "jshint": "~2.12",
      "merge-stream": "~2.0",
      "mocha": "~8.2",
      "typescript": "~4.1",
      "web-ignition": "~1.3"
   }
}
